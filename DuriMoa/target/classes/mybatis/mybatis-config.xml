<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
   <settings>
     <!-- Oracle 테이블의 컬럼이 mem_id 이고, DTO의 필드변수가 memId 일때 매칭이 가능하게 하는 옵션 -->
     <setting name="mapUnderscoreToCamelCase" value="true"/>
     <!-- 널허용 -->
     <setting name="jdbcTypeForNull" value="NULL"/> 
   </settings>
   <typeAliases>
      <typeAlias type="com.ugi.durimoa.member.vo.MemberVO" alias="MemberVO" />
      <typeAlias type="com.ugi.durimoa.member.vo.CoupleVO" alias="CoupleVO" />
      <typeAlias type="com.ugi.durimoa.member.vo.CoupleInfoVO" alias="CoupleInfoVO" />
      <typeAlias type="com.ugi.durimoa.member.vo.RequestVO" alias="RequestVO" />
      <typeAlias type="com.ugi.durimoa.travel.vo.TravelVO" alias="TravelVO" />
      <typeAlias type="com.ugi.durimoa.travel.vo.ImageVO" alias="ImageVO" />
      <typeAlias type="com.ugi.durimoa.travel.vo.TravelInfoVO" alias="TravelInfoVO" />
      <typeAlias type="com.ugi.durimoa.travel.vo.SearchVO" alias="SearchVO" />
      <typeAlias type="com.ugi.durimoa.board.vo.BoardVO" alias="BoardVO" />
      <typeAlias type="com.ugi.durimoa.board.vo.BimageVO" alias="BimageVO" />
      <typeAlias type="com.ugi.durimoa.board.vo.BoardInfoVO" alias="BoardInfoVO" />
      <typeAlias type="com.ugi.durimoa.board.vo.ReplyVO" alias="ReplyVO" />
      <typeAlias type="com.ugi.durimoa.board.vo.LikeVO" alias="LikeVO" />
   </typeAliases>
</configuration>